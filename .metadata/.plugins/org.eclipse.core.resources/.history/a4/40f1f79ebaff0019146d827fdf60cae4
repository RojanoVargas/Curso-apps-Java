
public class datosEnMayus {

	public static void main(String[] args) {

	}

	
	public static void imprimirDatos(String nombre, String apellidos, int edad, int telefono) {
		System.out.println("***************************");
		System.out.println("***" + nombre + apellidos + "***" + edad + "***" + telefono);
		System.out.println("***************************");
	}
}
