package avion;

import java.util.ArrayList;

public class Avion {
	private ArrayList <Pasajeros> pasajeros;
	private String tipoAvion;
	private ArrayList <Asientos> asientos;
	
	public ArrayList<Pasajeros> getPasajeros() {
		return pasajeros;
	}
	public void setPasajeros(ArrayList<Pasajeros> pasajeros) {
		this.pasajeros = pasajeros;
	}
	public String getTipoAvion() {
		return tipoAvion;
	}
	public void setTipoAvion(String tipoAvion) {
		this.tipoAvion = tipoAvion;
	}
	public ArrayList<Asientos> getAsientos() {
		return asientos;
	}
	public void setAsientos(ArrayList<Asientos> asientos) {
		this.asientos = asientos;
	}
	
	@Override
	public String toString() {
		return "Avion [pasajeros=" + pasajeros + ", tipoAvion=" + tipoAvion + ", asientos=" + asientos + "]";
	}
	
	
}
